{
  "name": "@cyberdrk/travel-concierge",
  "version": "1.0.1",
  "description": "Find contact details for accommodation listings (Airbnb, Booking.com, VRBO, Expedia)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "travel-concierge": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "travel",
    "concierge",
    "airbnb",
    "booking",
    "vrbo",
    "expedia",
    "contact",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyberdrk305/travel-concierge.git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "^12.0.0",
    "json5": "^2.2.3",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.25.0"
}
